{
    "hostRequirements": {
        "cpus": 4,
        "memory": "8gb",
        "storage": "32gb"
    },
    "image": "echipdockerhub/baseworkspace:spa-1.1.1",
    "name": "spa",
    "appPort": [
        "8001:8000"
    ]
    ,
    "extensions": [
		"vscjava.vscode-java-pack",
		"Angular.ng-template",
		"vscjava.vscode-gradle",
        "naumovs.color-highlight"
	]
    ,"mounts": [
        "source=${localEnv:HOME}${localEnv:USERPROFILE}/.m2,target=${containerEnv:HOME}${containerEnv:USERPROFILE}/.m2,type=bind,consistency=cached",
        "source=${localEnv:HOME}${localEnv:USERPROFILE}/.gradle,target=${containerEnv:HOME}${containerEnv:USERPROFILE}/.gradle,type=bind,consistency=cached"
    ]
}
